<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite" verbose="1" >
    <listeners>
        <listener class-name="com.qa.listeners.TestListener"/>
    </listeners>

    <parameter name="env" value="iOS_Flutter" />
    <parameter name="platformName" value="iOS" />
    <parameter name="platformVersion" value="15.5" />
    <parameter name="deviceName" value="iPhone 13" />
    <parameter name="wdaLocalPort" value="10001" />
    <parameter name="webkitDebugProxyPort" value="11001" />

    <test name="tests.iOS_Flutter">

        <classes>
            <class name="com.qa.tests.Home.HomePageTest"></class>
            <class name="com.qa.tests.Preview.PreviewPageTest"></class>
        </classes>
    </test>
</suite>
